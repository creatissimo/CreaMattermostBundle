{
  "name": "creatissimo/mattermost-bundle",
  "description": "Symfony Bundle for communicating with the Mattermost chat, sending messages on exceptions or when triggered by code",
  "keywords": [
    "exception",
    "mattermost"
  ],
  "homepage": "https://github.com/creatissimo/CreatissimoMattermostBundle",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Pascal Rossa",
      "email": "rossa@creatissimo.net"
    }
  ],
  "license": [
    "Apache-2.0"
  ],
  "require": {
    "php": ">=5.4.0",
    "symfony/framework-bundle": "~2.7|~3.0|~4.0",
    "ext-json": true,
    "ext-curl": true
  },
  "autoload": {
    "psr-4": {
      "Creatissimo\\MattermostBundle\\": ""
    }
  },
  "repositories": [],
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "minimum-stability": "dev"
}